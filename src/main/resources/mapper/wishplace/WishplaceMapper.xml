<?xml version="1.0" encoding="UTF-8"?>

<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN"
        "http://mybatis.org/dtd/mybatis-3-mapper.dtd">


<mapper namespace="com.SideProject.GALE.mapper.wishplace.WishplaceMapper">
	<insert id="Add" parameterType="HashMap">
		INSERT INTO board_wishplace
		(userid, board_number)
		VALUES
		(#{userid}, #{board_Number})
	</insert>
	
	<delete id="Del" parameterType="HashMap">
		DELETE
		FROM board_wishplace
		WHERE userid = #{userid} AND board_number = #{board_Number}
	</delete>
</mapper>